{
    "metadata": [
        {
            "src": [
                {
                    "files": [
                        "**/*.csproj"
                    ],
                    "exclude": [
                        "**/Test/**",
                        "**/bin/**",
                        "**/obj/**"
                    ],
                    "cwd": "FatturaElettronica.NET"
                }
            ],
            "dest": "obj/api/FatturaElettronica",
            "properties": {
                "TargetFramework": "netstandard2.0"
            }
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "**/*.yml"
                ],
                "cwd": "obj/api/FatturaElettronica",
                "dest": "api"
            },
            {
                "files": [
                    "**.md"
                ],
                "cwd": "FatturaElettronica.NET/docs",
                "dest": "docs"
            },
            {
                "files": [
                    "api/**.yml",
                    "api/index.md"
                ]
            },
            {
                "files": [
                    "articles/**.md",
                    "articles/**/toc.yml",
                    "toc.yml",
                    "*.md"
                ]
            },
            {
                "files": [
                    "*.md",
                    "dev/**.md",
                    "toc.yml"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**"
                ]
            }
        ],
        "overwrite": [
            {
                "files": [
                    "apidoc/**.md"
                ],
                "exclude": [
                    "obj/**",
                    "docs/**"
                ]
            }
        ],
        "dest": "docs",
        "globalMetadata": {
            "_appTitle": "Fattura Elettronica Open Source per piattaforme .NET",
            "_appFooter": "Â© Copyright 2015-2023 <a href=https://nicolaiarocci.com>Nicola Iarocci</a> e <a href=https://gestionaleamica.com/>Gestionali Amica</a>",
            "_appLogoPath": "images/logo.svg",
            "_appFaviconPath": "images/logo.png",
            "_enableSearch": true
        },
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "default",
            "modern",
            "templates/classic"
        ],
        "postProcessors": [],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false
    }
}
